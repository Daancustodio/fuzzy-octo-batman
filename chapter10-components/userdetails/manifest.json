{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "sapui5.demo.readingdata.userdetails",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"userData": {
				"uri": "local-service/userData.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "sapui5.demo.userdetails.view.UserDetails",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"userDetails": {
				"dataSource": "userData"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sapui5.demo.userdetails.i18n.i18n"
				}
			}
		}
	}
}